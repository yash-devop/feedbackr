{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "exports": {
    "./uiStyles.css": "./dist/uiStyles.css",
    "./*": "./src/*.tsx"
  },
  "scripts": {
    "build:ui": "npx @tailwindcss/cli -i ./src/uiStyles.css -o ./dist/uiStyles.css",
    "dev": "npx @tailwindcss/cli -i ./src/uiStyles.css -o ./dist/uiStyles.css --watch",
    "generate:component": "turbo gen react-component",
    "check-types": "tsc --noEmit"
  },
  "devDependencies": {
    "@repo/tailwind-config": "*",
    "@repo/typescript-config": "*",
    "@types/node": "^22.15.3",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.1",
    "react-hook-form": "^7.65.0",
    "typescript": "5.9.2"
  },
  "dependencies": {
    "@repo/utils": "*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "tailwind-merge": "^3.4.0",
    "tw-animate-css": "^1.4.0"
  }
}
